<%- css('/css/aboutme.css') %>
<%- js('/js/aboutme.js') %> 

<div id="container">
  <img
    src="/images/rev_sheep.png"
    id="mainIcon"
    class="icon"
    onclick="reverseIcon()"
  />
  <div id="signature">
    almost salted
  </div>
  <div id="contactPart">
    <% const keys=Object.keys(theme.contact);
    for (let i in keys){
      if(theme.contact[keys[i]].icon==="/images/contact/gmail.png"){ %>
        <div class="contactItem">
          <img src="<%= theme.contact[keys[i]].icon %>" />
          <a onclick="handleEmailClick()">
            <%= keys[i] %>
          </a>
        </div>
    <% } else { %>
      <div class="contactItem">
        <img src="<%= theme.contact[keys[i]].icon %>" />
        <a href="<%= theme.contact[keys[i]].url%>">
          <%= keys[i] %>
        </a>
      </div>
    <% }} %>
  </div>
</div>
